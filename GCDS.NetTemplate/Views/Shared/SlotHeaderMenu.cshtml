@using GCDS.NetTemplate.Components
@using GCDS.NetTemplate.Components.Gcds

@model ISlotHeaderMenu

@switch(Model)
{
	case GcdsTopicMenu topicMenu:
		@await Html.PartialAsync("Gcds/GcdsTopicMenu", topicMenu)
		break;
	case GcdsTopNav topNav:
		@await Html.PartialAsync("Gcds/GcdsTopNav", topNav)
		break;
}